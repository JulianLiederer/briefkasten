[program:briefkasten_worker]
command={{apphome}}/bin/worker -r /var/briefkasten/drop_root
user={{appuser}}
autostart=true
autorestart=true
startsecs=1
environment=PATH="/sbin:/bin:/usr/sbin:/usr/bin:/usr/local/sbin:/usr/local/bin"
stdout_logfile=/var/briefkasten/log/stdout.log
stderr_logfile=/var/briefkasten/log/error.log
#stdout_logfile=NONE
#stderr_logfile=NONE
